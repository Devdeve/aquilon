insert_job: unixeng-aqdrefreshwin-prod
machine: nyaqd1
job_type: c
command: /ms/dist/aurora/bin/krun -- /ms/dist/aquilon/PROJ/aqd/prod/bin/aq refresh windows hosts
date_conditions: yes
days_of_week: all
start_times: "6:50, 18:50"
permission: mx
std_out_file: /var/quattor/logs/$AUTO_JOB_NAME.out
std_err_file: /var/quattor/logs/$AUTO_JOB_NAME.err
alarm_if_fail: 1
description: "this job keeps the windows host information in aqdb up to date with the data warehouse"
